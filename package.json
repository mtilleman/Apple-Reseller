  "devDependencies": {
    "lite-server": "^2.6.1",
    "node-sass": "^5.0.0",
    "onchange": "^7.0.0",
    "parallelshell": "3.0.1"
  },
  "dependencies": {
    "bootstrap": "^5.0.2",
    "font-awesome": "^4.7.0"
  }
